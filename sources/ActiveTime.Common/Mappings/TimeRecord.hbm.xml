<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DustInTheWind.ActiveTime.Persistence.Entities" assembly="ActiveTime.Common">
  <class name="TimeRecord" table="records">
    <id name="Id" column="id" unsaved-value="0">
      <generator class="native"/>
    </id>
    <natural-id mutable="true">
      <property name="Date" column="date"/>
      <property name="StartTime" column="start_time" type="TimeAsTimeSpan"/>
      <property name="EndTime" column="end_time" type="TimeAsTimeSpan"/>
    </natural-id>
    <property name="RecordType" column="type"/>
    <!--<property name="RecordType" column="type" type="DustInTheWind.ActiveTime.Persistence.Entities.TimeRecordType, ActiveTime"/>-->
  </class>
</hibernate-mapping>